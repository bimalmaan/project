def fact(a):
    for i in range(2,int(a)+1):
        b = a%i
        if b==0:
            print(i)
    if a > 1:
        for i in range(2,int(a)):
            b = a%i
            if b == 0:
                break
        else:
            print(a, "is a prime number")
fact(int(input("Enter a number less than 1,00,000\n")))
